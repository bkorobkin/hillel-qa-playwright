{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Playwright Tests",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/node_modules/.bin/playwright",
        "args": ["test", "--project=chromium", "--headed"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "cwd": "${workspaceFolder}"
      }
    ]
  }
  